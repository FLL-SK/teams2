  "name": "teams2",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "start:dev": "yarn start:nx",
    "start": "yarn start:server",
    "start:nx": "yarn nx run-many --parallel --target=serve --projects=server,client ",
    "start:server": "node dist/apps/server/main.js",
    "build": "yarn build:server",
    "build:all": "yarn build:server && yarn build:client",
    "build:client": "yarn run-s generate build:clientapp",
    "build:server": "yarn run-s generate build:serverapp",
    "build:clientapp": "yarn nx run-many --target=build --projects=client",
    "build:serverapp": "yarn nx run-many --target=build --projects=server",
    "test": "yarn nx test",
    "generate": "yarn run-s build:contract generate:contract",
    "generate:contract": "graphql-codegen --config codegen.yml",
    "build:contract": "ts-node ./scripts/build-schema.ts",
    "testseed": "yarn nx run server:testseed"
  },
  "private": true,
