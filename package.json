{
  "name": "teams2",
  "version": "0.0.0",
  "license": "MIT",
  "packageManager": "yarn@4.3.1",
  "engines": {
    "node": ">=22",
    "yarn": ">=4"
  },
  "scripts": {
    "start": "yarn start:server",
    "start:dev": "yarn nx run-many --parallel --target=serve --projects=server,client",
    "start:server": "node dist/apps/server/main.js",
    "build": "yarn build:all",
    "build:all": "yarn generate && yarn compile",
    "build:client": "yarn generate && yarn compile:client",
    "build:server": "yarn generate && yarn compile:server",
    "compile:client": "yarn nx run-many --target=build --projects=client",
    "compile:server": "yarn nx run-many --target=build --projects=server",
    "compile": "yarn nx run-many --target=build --projects=server,client --no-cloud",
    "test": "yarn nx test",
    "generate": "yarn run-s gql:schema gql:codegen",
    "gql:codegen": "graphql-codegen --config codegen.yaml",
    "gql:schema": "ts-node ./scripts/build-schema.ts",
    "testseed": "yarn nx run server:testseed"
  },
  "private": true,
  "resolutions": {
    "@types/react": "18.3.18",
    "@types/react-dom": "18.3.5"
  },
